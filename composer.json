{
    "name": "laravel-duckdb/duckdb-driver",
    "description": "A Laravel database driver for DuckDB.",
    "keywords": [
        "laravel",
        "duckdb",
        "database",
        "driver",
        "ffi",
        "olap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Filipe Fernandes",
            "email": "email@example.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^10.0|^11.0",
        "illuminate/support": "^10.0|^11.0",
        "satur/duckdb-auto": "^0.1"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelDuckDB\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelDuckDB\\DuckDBServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
