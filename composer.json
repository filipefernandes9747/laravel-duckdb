{
    "name": "filipefernandes/laravel-duckdb",
    "description": "A Laravel database driver for DuckDB.",
    "keywords": [
        "laravel",
        "duckdb",
        "database",
        "driver",
        "ffi",
        "olap"
    ],
    "license": "MIT",
    "authors": [
        {
            "name": "Filipe Fernandes",
            "email": "filipe9747@gmail.com"
        }
    ],
    "require": {
        "php": "^8.2",
        "illuminate/database": "^10.0|^11.0|^12.0",
        "illuminate/support": "^10.0|^11.0|^12.0",
        "satur.io/duckdb-auto": "^2.0"
    },
    "require-dev": {
        "orchestra/testbench": "^8.0|^9.0",
        "phpunit/phpunit": "^10.0"
    },
    "autoload": {
        "psr-4": {
            "LaravelDuckDB\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "LaravelDuckDB\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "LaravelDuckDB\\DuckDBServiceProvider"
            ]
        }
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "php-http/discovery": true,
            "satur.io/duckdb-auto": true
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}